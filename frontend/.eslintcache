173","174"],"/home/ftdgomez/projects/inventario/frontend/src/actions/userActions.js",[],["175","176"],"/home/ftdgomez/projects/inventario/frontend/src/screens/LoginScreen.js",["177","178","179","180"],"import React, { useState, useEffect } from 'react'\nimport { Link } from 'react-router-dom'\nimport { Form, Button, Row, Col } from 'react-bootstrap'\nimport { useDispatch, useSelector } from 'react-redux'\n// import Message from '../components/Message'\nimport Loader from '../components/Loader'\nimport FormContainer from '../components/FormContainer'\nimport { login } from '../actions/userActions'\nimport logo from '../assets/icon.png'\n\nconst LoginScreen = ({ location, history }) => {\n  const [email, setEmail] = useState('')\n  const [password, setPassword] = useState('')\n\n  const dispatch = useDispatch()\n\n  const userLogin = useSelector((state) => state.userLogin)\n  const { loading, error, userInfo } = userLogin\n\n  const redirect = location.search ? location.search.split('=')[1] : '/'\n\n  useEffect(() => {\n    if (userInfo) {\n      history.push(redirect)\n    }\n  }, [history, userInfo, redirect])\n\n  const submitHandler = (e) => {\n    e.preventDefault()\n    dispatch(login(email, password))\n  }\n\n  return (\n    <FormContainer>\n      <img src={logo} alt=\"logo\" style={{height: '32px', width: '32px'}} />\n      <h4>Iniciar Sesión</h4>\n      {/* {error && <Message variant='danger'>{error}</Message>} */}\n      {loading && <Loader />}\n      <Form onSubmit={submitHandler}>\n        <Form.Group controlId='email'>\n          <Form.Label>Dirección de Email</Form.Label>\n          <Form.Control\n            type='email'\n            placeholder='Su email'\n            value={email}\n            onChange={(e) => setEmail(e.target.value)}\n          ></Form.Control>\n        </Form.Group>\n\n        <Form.Group controlId='password'>\n          <Form.Label>Contraseña</Form.Label>\n          <Form.Control\n            type='password'\n            placeholder='Su contraseña'\n            value={password}\n            onChange={(e) => setPassword(e.target.value)}\n          ></Form.Control>\n        </Form.Group>\n\n        <Button type='submit' variant='primary'>\n          Iniciar sesión\n        </Button>\n      </Form>\n    </FormContainer>\n  )\n}\n\nexport default LoginScreen\n",["181","182"],"/home/ftdgomez/projects/inventario/frontend/src/reducers/userReducers.js",[],["183","184"],"/home/ftdgomez/projects/inventario/frontend/src/constants/userConstants.js",[],"/home/ftdgomez/projects/inventario/frontend/src/layouts/MainLayout.js",[],"/home/ftdgomez/projects/inventario/frontend/src/components/Paginate.js",["185","186"],"import React, { useState } from 'react'\nimport { Pagination } from 'react-bootstrap'\nimport {Link} from 'react-router-dom'\nimport { LinkContainer } from 'react-router-bootstrap'\n\nconst Paginate = ({ pages, page, isAdmin = false, keyword = '' }) => {\n\n  return (\n    pages > 1 && (\n      <Pagination>\n\n        <LinkContainer to={`/page/1`}>\n          <Pagination.Item>\n            <small><i className=\"im im-previous\" style={{fontSize: 14}}></i></small>\n          </Pagination.Item>\n        </LinkContainer>  \n\n        <LinkContainer to={`/page/${page - 1}`}>\n          <Pagination.Item>\n            <small><i className=\"im im-care-left\" style={{fontSize: 14}}></i></small>\n          </Pagination.Item>\n        </LinkContainer>\n\n\n        <LinkContainer to={`/page/${page}`}>\n          <Pagination.Item active={true}>{page}</Pagination.Item>\n        </LinkContainer>\n\n        <LinkContainer to={`/page/${page + 1}`}>\n          <Pagination.Item>\n            <small><i className=\"im im-care-right\" style={{fontSize: 14}}></i></small>\n          </Pagination.Item>\n        </LinkContainer>\n\n        <LinkContainer to={`/page/${pages}`}>\n          <Pagination.Item>\n            <small><i className=\"im im-next\" style={{fontSize: 14}}></i></small>\n          </Pagination.Item>\n        </LinkContainer>  \n\n{/*         {[...Array(pages).keys()].map((x) => (\n          <LinkContainer\n            key={x + 1}\n            to={\n              !isAdmin\n                ? keyword\n                  ? `/search/${keyword}/page/${x + 1}`\n                  : `/page/${x + 1}`\n                : `/admin/productlist/${x + 1}`\n            }\n          >\n            <Pagination.Item active={x + 1 === page}>{x + 1}</Pagination.Item>\n          </LinkContainer>\n        ))} */}\n      </Pagination>\n    )\n  )\n}\n\nexport default Paginate\n","/home/ftdgomez/projects/inventario/frontend/src/components/FormContainer.js",["187","188"],"import React from 'react'\nimport { Container, Row, Col } from 'react-bootstrap'\n\nconst FormContainer = ({ children }) => {\n  return (\n    <Container className=\"min-vh-100 d-flex align-items-center justify-content-center\">\n          <div className=\"card p-4 rounded-xl\">{children}</div>\n    </Container>\n  )\n}\n\nexport default FormContainer\n","/home/ftdgomez/projects/inventario/frontend/src/screens/NotFoundScreen.js",[],"/home/ftdgomez/projects/inventario/frontend/src/store.js",[],"/home/ftdgomez/projects/inventario/frontend/src/screens/ProductHandler.js",["189"],"import React, { useState, useEffect} from 'react'\nimport {useSelector, useDispatch} from 'react-redux'\nimport { createProduct } from '../actions/productActions'\nimport MainLayout from '../layouts/MainLayout'\nimport { Form, Col, Button, Toast } from 'react-bootstrap'\nimport { Link } from 'react-router-dom'\n\nconst ProductHandler = () => {\n  const [name, setName] = useState('')\n  const [brand, setBrand] = useState('')\n  const [description, setDescription] = useState('')\n  const [tagsStr, setTags] = useState('')\n\n  const [isVariant, setForVariant] = useState(false)\n  const [variants, setVariants] = useState([{ref: 'main', sellPrice: '', stock: ''}])\n\n  const [success, setSuccess] = useState(false)\n\n  const handleVariantChange = (e, index, key) => {\n      let tempArr = []\n      variants.forEach((el, i) => {\n        if (i !== index)\n        {\n          tempArr.push(el)\n        }\n        else\n        {\n          tempArr.push({...el, [key]: e.target.value})\n        }\n      });\n      setVariants(tempArr)\n  }\n\n  const dispatch = useDispatch()\n  const productCreate = useSelector((state) => state.productCreate )\n\n  const submitHandler = (e) => {\n    e.preventDefault()\n    let tags = tagsStr.split(',').map(el => el.trim())\n    dispatch(createProduct({\n      name,\n      brand,\n      description,\n      tags,\n      variants\n    }))\n    if (productCreate.success)\n    {\n      setName('')\n      setBrand('')\n      setDescription('')\n      setTags('')\n      setForVariant(false)\n      setVariants([{ref: 'main', sellPrice: '', stock: ''}])\n      setSuccess(true)\n    }\n    else\n    {\n      alert('Ha ocurrido un error, revise la data.')\n    }\n  }\n\n  return (\n      <MainLayout>\n        <div style={{height: '97vh', overflowY: 'auto'}} className=\"main-container bg-transparent\">\n        <div className=\"border-bottom d-flex align-items-center mb-4 p-4\">\n          <Link to='/' className='btn btn-light border my-3'>\n            {'<'}\n          </Link>\n          <header className=\"ml-2 pt-2\">\n              <h4>Crear Producto Nuevo</h4>\n          </header>\n        </div>\n\n        <Toast onClose={() => setSuccess(false)} show={success} delay={3000} autohide>\n          <Toast.Body>Se ha creado un producto con éxito.</Toast.Body>\n        </Toast>\n\n        <Form className=\"px-4\" style={{maxWidth: '700px'}} onSubmit={submitHandler}>\n\n          <Form.Row className=\"p-4 border rounded-xl mb-4 bg-white shadow-sm\">\n            <Col sm={12}><h4><small>Título, marca y descripción.</small></h4></Col>\n            <Col md={7}>\n              <Form.Control onChange={(e)=> setName(e.target.value)} value={name} placeholder=\"Nombre de producto\" />\n            </Col>\n            <Col>\n              <Form.Control onChange={(e)=> setBrand(e.target.value)} value={brand} placeholder=\"Marca\" />\n            </Col>\n            <Col sm={12}>\n              <Form.Control onChange={(e)=> setDescription(e.target.value)} value={description}  as=\"textarea\" placeholder=\"Descripción de producto\" className=\"mt-2\" />\n              <Form.Text className=\"text-muted\">\n                Solo se utiliza para la descripción en la tienda online.\n              </Form.Text>\n            </Col>\n          </Form.Row>\n\n          <div className=\"p-4 border rounded-xl mb-4 bg-white shadow-sm\">\n            <h4><small>Tags</small></h4>\n            <p>Escriba los tags separados por comas (,)</p>\n            <Form.Control onChange={(e)=> setTags(e.target.value)} value={tagsStr} placeholder=\"tag1, tag2, tag3, tag4\" />\n            <Form.Text className=\"text-muted\">\n              Los tags funcionan como palabras claves en el buscador. En la tienda web, también se utilizarán para palablas claves en buscadores (SEO)\n            </Form.Text>\n          </div>\n\n          <Form.Row className=\"p-4 border rounded-xl mb-4 bg-white shadow-sm\">\n            <Col sm={12} className=\"mt-2\">\n              <Form.Group controlId=\"variantHandler\" onChange={()=>setForVariant(!isVariant)}>\n                <Form.Check style={{color: '#646464'}} type=\"checkbox\" label=\"Este producto tiene diferentes opciones, como distintos tamaños o colores.\" />\n              </Form.Group>\n            </Col>\n          </Form.Row>\n          <div className=\"p-4 border rounded-xl mb-4 bg-white shadow-sm\">\n            {\n              !isVariant ? <h4><small>Precio y Stock</small></h4> : <h4><small>Referencia variante, precio de variante y stock de variante</small></h4>\n            }\n\n            {\n              isVariant\n              ?\n                <div>\n                  {\n                    variants.map((v, index) => (\n                      <Form.Row className=\"mb-2\" key={`ref-item-${index}`}>\n                          <Col>\n                            <Form.Control onChange={(e)=>handleVariantChange(e, index, 'ref')} placeholder=\"Ref\" value={v.ref} />\n                          </Col>\n                          <Col>\n                            <Form.Control onChange={(e)=>handleVariantChange(e, index, 'sellPrice')} placeholder=\"Precio\" value={v.sellPrice} />\n                          </Col>\n                          <Col>\n                            <Form.Control onChange={(e)=>handleVariantChange(e, index, 'stock')} type=\"number\" placeholder=\"stock\" value={v.stock} />\n                          </Col>\n                          <Col sm={1}>\n                            {index > 0 && <Button onClick={()=>setVariants(variants.filter((variants, i) => i !== index))} variant=\"danger\">x</Button>}\n                          </Col>\n                      </Form.Row>\n                    ))\n                  }\n\n                  <Button onClick={()=>setVariants([...variants, {ref: '', sellPrice: '', stock: ''}])} className=\"mt-4\"><i className=\"im im-plus mr-2\" style={{fontSize: '12px'}}></i>Agregar Variante</Button>\n                </div>\n              :\n                <Form.Row>\n                  <Col md={7}>\n                    <Form.Control onChange={(e)=>setVariants([{...variants[0], sellPrice: e.target.value}])} value={variants[0].sellPrice} placeholder=\"Precio\" />\n                  </Col>\n                  <Col>\n                    <Form.Control onChange={(e)=>setVariants([{...variants[0], stock: e.target.value}])} value={variants[0].stock} type=\"number\" placeholder=\"999\" />\n                  </Col>\n                </Form.Row>\n            }\n          </div>\n          <Button className=\"mb-4\" type=\"submit\" variant=\"primary\" size=\"sm\">Crear Producto</Button>\n        </Form>\n        </div>\n      </MainLayout>\n  )\n}\n\nexport default ProductHandler\n",["190","191"],"/home/ftdgomez/projects/inventario/frontend/src/screens/EditProduct.js",["192","193","194","195","196","197"],"import React, { useState, useEffect} from 'react'\nimport {useSelector, useDispatch} from 'react-redux'\nimport { listProductDetails, updateProduct } from '../actions/productActions'\nimport MainLayout from '../layouts/MainLayout'\nimport { Form, Col, Button, Toast } from 'react-bootstrap'\nimport { Link } from 'react-router-dom'\nimport Loader from '../components/Loader'\nimport { PRODUCT_UPDATE_RESET } from '../constants/productConstants'\n\nconst EditProduct = ({ match, history }) => {\n\n  const productId = match.params.id\n  const [name, setName] = useState('')\n  const [brand, setBrand] = useState('')\n  const [description, setDescription] = useState('')\n  const [tagsStr, setTags] = useState('')\n  const [variants, setVariants] = useState([{ref: 'main', sellPrice: '', stock: ''}])\n\n  const [success, setSuccess] = useState(false)\n\n  const dispatch = useDispatch()\n\n  const productDetails = useSelector((state) => state.productDetails)\n  const { loading, error, product } = productDetails\n\n  const productUpdate = useSelector((state) => state.productUpdate)\n  const {\n    loading: loadingUpdate,\n    error: errorUpdate,\n    success: successUpdate,\n  } = productUpdate\n\n\n  useEffect(() => {\n    if (successUpdate) {\n      dispatch({ type: PRODUCT_UPDATE_RESET })\n      history.push('/')\n    } \n    else\n    {\n      if (!product.name){\n        dispatch(listProductDetails(productId))\n      }\n      else\n      {\n        setName(product.name)\n        setBrand(product.brand)\n        setDescription(product.description)\n        setTags(product.tags.toString())\n        setVariants(product.variants)\n      }\n    }\n  }, [dispatch, history, productId, product, successUpdate])\n\n  const submitHandler = (e) => {\n    e.preventDefault()\n    dispatch(\n      updateProduct({\n        _id: productId,\n        name,\n        brand,\n        description,\n        tags: tagsStr.split(',').map(el => el.trim()),\n        variants\n      })\n    )\n  }\n\n  if (loading)\n  {\n    return <Loader />\n  }\n  return (\n      <MainLayout>\n        <div style={{height: '97vh', overflowY: 'auto'}} className=\"main-container bg-transparent\">\n        <div className=\"border-bottom d-flex align-items-center mb-4 p-4\">\n          <Link to='/' className='btn btn-light border my-3'>\n            {'<'}\n          </Link>\n          <header className=\"ml-2 pt-2\">\n              <h4>Editar Producto</h4>\n          </header>\n        </div>\n\n     {/*    <Toast onClose={() => setSuccess(false)} show={success} delay={3000} autohide>\n          <Toast.Body>Se ha creado un producto con éxito.</Toast.Body>\n        </Toast> */}\n\n        <Form onSubmit={submitHandler} className=\"px-4\" style={{maxWidth: '700px'}} >\n\n          <Form.Row className=\"p-4 border rounded-xl mb-4 bg-white shadow-sm\">\n            <Col sm={12}><h4><small>Título, marca y descripción.</small></h4></Col>\n            <Col md={7}>\n              <Form.Control onChange={(e)=> setName(e.target.value)} value={name} placeholder=\"Nombre de producto\" />\n            </Col>\n            <Col>\n              <Form.Control onChange={(e)=> setBrand(e.target.value)} value={brand} placeholder=\"Marca\" />\n            </Col>\n            <Col sm={12}>\n              <Form.Control onChange={(e)=> setDescription(e.target.value)} value={description}  as=\"textarea\" placeholder=\"Descripción de producto\" className=\"mt-2\" />\n              <Form.Text className=\"text-muted\">\n                Solo se utiliza para la descripción en la tienda online.\n              </Form.Text>\n            </Col>\n          </Form.Row>\n\n          <div className=\"p-4 border rounded-xl mb-4 bg-white shadow-sm\">\n            <h4><small>Tags</small></h4>\n            <p>Escriba los tags separados por comas (,)</p>\n            <Form.Control onChange={(e)=> setTags(e.target.value)} value={tagsStr} placeholder=\"tag1, tag2, tag3, tag4\" />\n            <Form.Text className=\"text-muted\">\n              Los tags funcionan como palabras claves en el buscador. En la tienda web, también se utilizarán para palablas claves en buscadores (SEO)\n            </Form.Text>\n          </div>\n\n          <div className=\"p-4 border rounded-xl mb-4 bg-white shadow-sm\">\n            {\n              variants.length < 1 ? <h4><small>Precio y Stock</small></h4> : <h4><small>Referencia variante, precio de variante y stock de variante</small></h4>\n            }\n\n              <div>\n                  {\n                    variants.map((v, index) => (\n                      <Form.Row className=\"mb-2\" key={`ref-item-${index}`}>\n                          <Col>\n                            <Form.Control placeholder=\"Ref\" value={v.ref} />\n                          </Col>\n                          <Col>\n                            <Form.Control placeholder=\"Precio\" value={v.sellPrice} />\n                          </Col>\n                          <Col>\n                            <Form.Control type=\"number\" placeholder=\"stock\" value={v.stock} />\n                          </Col>\n                          <Col sm={1}>\n                            {index > 0 && <Button onClick={()=>setVariants(variants.filter((variants, i) => i !== index))} variant=\"danger\">x</Button>}\n                          </Col>\n                      </Form.Row>\n                    ))\n                  }\n\n                  <Button onClick={()=>setVariants([...variants, {ref: '', sellPrice: '', stock: ''}])} className=\"mt-4\"><i className=\"im im-plus mr-2\" style={{fontSize: '12px'}}></i>Agregar Variante</Button>\n                </div>\n          </div>\n          <Button className=\"mb-4\" type=\"submit\" variant=\"primary\" size=\"sm\">Actualizar Producto</Button>\n        </Form>\n        </div>\n      </MainLayout>\n  )\n}\n\nexport default E